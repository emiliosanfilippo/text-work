    <!-- http://www.textualpatterns.org#hasPublicationYear -->

    <owl:DatatypeProperty rdf:about="http://www.textualpatterns.org#hasPublicationYear">
        <rdfs:domain rdf:resource="http://www.textualpatterns.org#PublicationSituation"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
                    <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#gYear"/>
                    <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#gYearMonth"/>
                </owl:unionOf>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:label>has publication year</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.textualpatterns.org#hasTitle -->

    <owl:DatatypeProperty rdf:about="http://www.textualpatterns.org#hasTitle">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.textualpatterns.org#DocumentaryWork"/>
                    <rdf:Description rdf:about="http://www.textualpatterns.org#Text"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>has title</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.textualpatterns.org#DocumentaryWork -->

    <owl:Class rdf:about="http://www.textualpatterns.org#DocumentaryWork">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.textualpatterns.org#groups"/>
                <owl:someValuesFrom rdf:resource="http://www.textualpatterns.org#Text"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.textualpatterns.org#groups"/>
                <owl:allValuesFrom rdf:resource="http://www.textualpatterns.org#Text"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.textualpatterns.org#Person"/>
        <owl:disjointWith rdf:resource="http://www.textualpatterns.org#Publisher"/>
        <owl:disjointWith rdf:resource="http://www.textualpatterns.org#Situation"/>
        <owl:disjointWith rdf:resource="http://www.textualpatterns.org#Text"/>
        <owl:disjointWith rdf:resource="http://www.textualpatterns.org#VariantType"/>
        <rdfs:label>Documentary Work</rdfs:label>
    </owl:Class>
    


    <!-- http://www.textualpatterns.org#Person -->

    <owl:Class rdf:about="http://www.textualpatterns.org#Person">
        <owl:disjointWith rdf:resource="http://www.textualpatterns.org#Publisher"/>
        <owl:disjointWith rdf:resource="http://www.textualpatterns.org#Situation"/>
        <owl:disjointWith rdf:resource="http://www.textualpatterns.org#Text"/>
        <owl:disjointWith rdf:resource="http://www.textualpatterns.org#VariantType"/>
        <rdfs:label>Person</rdfs:label>
    </owl:Class>
    


    <!-- http://www.textualpatterns.org#PublicationSituation -->

    <owl:Class rdf:about="http://www.textualpatterns.org#PublicationSituation">
        <rdfs:subClassOf rdf:resource="http://www.textualpatterns.org#Situation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.textualpatterns.org#hasPublisher"/>
                <owl:someValuesFrom rdf:resource="http://www.textualpatterns.org#Publisher"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.textualpatterns.org#situationOf"/>
                <owl:someValuesFrom rdf:resource="http://www.textualpatterns.org#Text"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.textualpatterns.org#hasPublisher"/>
                <owl:allValuesFrom rdf:resource="http://www.textualpatterns.org#Publisher"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>Publication Situation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.textualpatterns.org#Publisher -->

    <owl:Class rdf:about="http://www.textualpatterns.org#Publisher">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.textualpatterns.org#publisherOf"/>
                <owl:someValuesFrom rdf:resource="http://www.textualpatterns.org#PublicationSituation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.textualpatterns.org#publisherOf"/>
                <owl:allValuesFrom rdf:resource="http://www.textualpatterns.org#PublicationSituation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.textualpatterns.org#Situation"/>
        <owl:disjointWith rdf:resource="http://www.textualpatterns.org#Text"/>
        <owl:disjointWith rdf:resource="http://www.textualpatterns.org#VariantType"/>
        <rdfs:label>Publisher</rdfs:label>
    </owl:Class>
    


    <!-- http://www.textualpatterns.org#Situation -->

    <owl:Class rdf:about="http://www.textualpatterns.org#Situation">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.textualpatterns.org#includedIn"/>
                <owl:allValuesFrom rdf:resource="http://www.textualpatterns.org#Situation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.textualpatterns.org#includes"/>
                <owl:allValuesFrom rdf:resource="http://www.textualpatterns.org#Situation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.textualpatterns.org#Text"/>
        <owl:disjointWith rdf:resource="http://www.textualpatterns.org#VariantType"/>
        <rdfs:label>Situation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.textualpatterns.org#Text -->

    <owl:Class rdf:about="http://www.textualpatterns.org#Text">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.textualpatterns.org#hasAuthor"/>
                <owl:someValuesFrom rdf:resource="http://www.textualpatterns.org#Person"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.textualpatterns.org#groupedBy"/>
                <owl:allValuesFrom rdf:resource="http://www.textualpatterns.org#DocumentaryWork"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.textualpatterns.org#hasSituation"/>
                <owl:allValuesFrom rdf:resource="http://www.textualpatterns.org#Situation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.textualpatterns.org#hasVariantType"/>
                <owl:allValuesFrom rdf:resource="http://www.textualpatterns.org#VariantType"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.textualpatterns.org#includedIn"/>
                <owl:allValuesFrom rdf:resource="http://www.textualpatterns.org#Text"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.textualpatterns.org#includes"/>
                <owl:allValuesFrom rdf:resource="http://www.textualpatterns.org#Text"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.textualpatterns.org#VariantType"/>
        <rdfs:label>Text</rdfs:label>
    </owl:Class>
    


    <!-- http://www.textualpatterns.org#VariantType -->

    <owl:Class rdf:about="http://www.textualpatterns.org#VariantType">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.textualpatterns.org#variantTypeOf"/>
                <owl:someValuesFrom rdf:resource="http://www.textualpatterns.org#Text"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>Variant Type</rdfs:label>
    </owl:Class>
    


