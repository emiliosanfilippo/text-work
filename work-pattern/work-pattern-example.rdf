    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.textualpatterns.org#authorOf -->

    <owl:ObjectProperty rdf:about="http://www.textualpatterns.org#authorOf">
        <owl:inverseOf rdf:resource="http://www.textualpatterns.org#hasAuthor"/>
        <rdfs:domain rdf:resource="http://www.textualpatterns.org#Person"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.textualpatterns.org#DocumentaryWork"/>
                    <rdf:Description rdf:about="http://www.textualpatterns.org#Text"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label>author of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.textualpatterns.org#groupedBy -->

    <owl:ObjectProperty rdf:about="http://www.textualpatterns.org#groupedBy">
        <owl:inverseOf rdf:resource="http://www.textualpatterns.org#groups"/>
        <rdfs:domain rdf:resource="http://www.textualpatterns.org#Text"/>
        <rdfs:range rdf:resource="http://www.textualpatterns.org#DocumentaryWork"/>
        <rdfs:label>grouped by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.textualpatterns.org#groups -->

    <owl:ObjectProperty rdf:about="http://www.textualpatterns.org#groups">
        <rdfs:domain rdf:resource="http://www.textualpatterns.org#DocumentaryWork"/>
        <rdfs:range rdf:resource="http://www.textualpatterns.org#Text"/>
        <rdfs:label>groups</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.textualpatterns.org#hasAuthor -->

    <owl:ObjectProperty rdf:about="http://www.textualpatterns.org#hasAuthor">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.textualpatterns.org#DocumentaryWork"/>
                    <rdf:Description rdf:about="http://www.textualpatterns.org#Text"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.textualpatterns.org#Person"/>
        <rdfs:label>has author</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.textualpatterns.org#hasPublisher -->

    <owl:ObjectProperty rdf:about="http://www.textualpatterns.org#hasPublisher">
        <owl:inverseOf rdf:resource="http://www.textualpatterns.org#publisherOf"/>
        <rdfs:domain rdf:resource="http://www.textualpatterns.org#PublicationSituation"/>
        <rdfs:range rdf:resource="http://www.textualpatterns.org#Publisher"/>
        <rdfs:label>has publisher</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.textualpatterns.org#hasSituation -->

    <owl:ObjectProperty rdf:about="http://www.textualpatterns.org#hasSituation">
        <rdfs:range rdf:resource="http://www.textualpatterns.org#Situation"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.textualpatterns.org#includedIn"/>
            <rdf:Description rdf:about="http://www.textualpatterns.org#hasSituation"/>
        </owl:propertyChainAxiom>
        <rdfs:label>has situation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.textualpatterns.org#hasVariantType -->

    <owl:ObjectProperty rdf:about="http://www.textualpatterns.org#hasVariantType">
        <owl:inverseOf rdf:resource="http://www.textualpatterns.org#variantTypeOf"/>
        <rdfs:domain rdf:resource="http://www.textualpatterns.org#Text"/>
        <rdfs:range rdf:resource="http://www.textualpatterns.org#VariantType"/>
        <rdfs:label>has variant type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.textualpatterns.org#includedIn -->

    <owl:ObjectProperty rdf:about="http://www.textualpatterns.org#includedIn">
        <owl:inverseOf rdf:resource="http://www.textualpatterns.org#includes"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.textualpatterns.org#DocumentaryWork"/>
                    <rdf:Description rdf:about="http://www.textualpatterns.org#Situation"/>
                    <rdf:Description rdf:about="http://www.textualpatterns.org#Text"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.textualpatterns.org#DocumentaryWork"/>
                    <rdf:Description rdf:about="http://www.textualpatterns.org#Situation"/>
                    <rdf:Description rdf:about="http://www.textualpatterns.org#Text"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label>included in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.textualpatterns.org#includes -->

    <owl:ObjectProperty rdf:about="http://www.textualpatterns.org#includes">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.textualpatterns.org#DocumentaryWork"/>
                    <rdf:Description rdf:about="http://www.textualpatterns.org#Situation"/>
                    <rdf:Description rdf:about="http://www.textualpatterns.org#Text"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.textualpatterns.org#DocumentaryWork"/>
                    <rdf:Description rdf:about="http://www.textualpatterns.org#Situation"/>
                    <rdf:Description rdf:about="http://www.textualpatterns.org#Text"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label>includes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.textualpatterns.org#publisherOf -->

    <owl:ObjectProperty rdf:about="http://www.textualpatterns.org#publisherOf">
        <rdfs:domain rdf:resource="http://www.textualpatterns.org#Publisher"/>
        <rdfs:range rdf:resource="http://www.textualpatterns.org#PublicationSituation"/>
        <rdfs:label>publisher of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.textualpatterns.org#situationOf -->

    <owl:ObjectProperty rdf:about="http://www.textualpatterns.org#situationOf">
        <rdfs:domain rdf:resource="http://www.textualpatterns.org#Situation"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.textualpatterns.org#situationOf"/>
            <rdf:Description rdf:about="http://www.textualpatterns.org#includes"/>
        </owl:propertyChainAxiom>
        <rdfs:label>situation of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.textualpatterns.org#variantTypeOf -->

    <owl:ObjectProperty rdf:about="http://www.textualpatterns.org#variantTypeOf">
        <rdfs:domain rdf:resource="http://www.textualpatterns.org#VariantType"/>
        <rdfs:range rdf:resource="http://www.textualpatterns.org#Text"/>
    </owl:ObjectProperty>
    


